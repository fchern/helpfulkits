:imap jj <Esc>
:set number relativenumber

color desert
set tabstop=4
set shiftwidth=4  
set expandtab 

set guifont=Monospace\ 13

nnoremap <silent> [b :bprevious<CR>
nnoremap <silent> ]b :bnext<CR>
nnoremap <silent> [B :bfirst<CR>
nnoremap <silent> [B :blast<CR>

#set runtimepath^=~/.vim/bundle/ctrlp.vim
